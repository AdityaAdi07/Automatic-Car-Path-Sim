{
  "rewrites": [
    { "source": "/", "destination": "/landing/index.html" },
    { "source": "/landing/(.*)", "destination": "/landing/$1" },
    { "source": "/app", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite"
}